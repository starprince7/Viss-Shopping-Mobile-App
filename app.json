{
  "expo": {
    "name": "Viss Store",
    "slug": "viss-shopping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/appstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/viss-splash-screen.gif",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/playstore.png",
        "backgroundColor": "#000000"
      },
      "package": "com.vissstore"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "26a3e68d-d50f-4691-8f8f-8c60bea65b43"
      }
    }
  }
}
